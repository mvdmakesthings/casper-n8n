# Tailscale
TS_AUTHKEY=tskey-auth-xxxxx          # Generate at https://login.tailscale.com/admin/settings/keys
TS_HOSTNAME=n8n-gpu                # Node name on your tailnet

# n8n
N8N_ENCRYPTION_KEY=                  # Random string — used to encrypt credentials in the DB
N8N_USER_MANAGEMENT_JWT_SECRET=      # Random string — used for session tokens
WEBHOOK_URL=https://n8n-gpu.<your-tailnet>.ts.net  # Tailscale HTTPS URL for webhook callbacks

# n8n-patched (enterprise dev bypass)
N8N_VERSION=2.8.3                    # Pinned n8n version — must match a patch-compatible release
NODE_VERSION=22                      # Node.js major version for the build
N8N_PATCHED_ENCRYPTION_KEY=          # Random string — separate from the main n8n instance
N8N_PATCHED_JWT_SECRET=              # Random string — separate from the main n8n instance
WEBHOOK_URL_PATCHED=http://localhost:5679  # Patched instance webhook URL (host port)
